{
  "applicationName": "My application",
  "properties": {
    "sizes": [
      {
        "name": "text_field_size",
        "width": 260,
        "height": 20
      },
      {
        "name": "text_area_size",
        "width": 260,
        "height": 200
      },
      {
        "name": "check_box_size",
        "width": 200,
        "height": 20
      },
      {
        "name": "slider_size",
        "width": 350,
        "height": 20
      },
      {
        "name": "label_size",
        "width": 100,
        "height": 20
      },
      {
        "name": "list_size",
        "width": 260,
        "height": 60
      },
      {
        "name": "button_size",
        "width": 120,
        "height": 30
      }
    ],
    "colors": [
      {
        "name": "panel_bg",
        "type": "background",
        "value": "FFC6AC"
      },
      {
        "name": "text_field_bg",
        "type": "background",
        "value": "FFF6DC"
      },
      {
        "name": "slider_fg",
        "type": "foreground",
        "value": "#FFFFFF"
      },
      {
        "name": "control_background",
        "type": "background",
        "value": "9E9FA5"
      },
      {
        "name": "control_foreground",
        "type": "foreground",
        "value": "#FFFFFF"
      }
    ],
    "fonts": [
      {
        "name": "content_font",
        "fontName": "Arial",
        "fontSize": 12,
        "fontStyle": "bold"
      },
      {
        "name": "text_field_font",
        "fontName": "Arial",
        "fontSize": 13
      }
    ]
  },
  "frames": [
    {
      "name": "customer_request_window",
      "title": "Customer requests",
      "panels": [
        {
          "fromTheTopOfFrame": true,
          "properties": [
            "panel_bg",
            "content_font"
          ],
          "elements": [
            {
              "type": "label",
              "title": "Name:",
              "laying": "row",
              "properties": [
                "label_size"
              ]
            },
            {
              "type": "textField",
              "name": "name_field",
              "laying": "row",
              "properties": [
                "text_field_size",
                "text_field_bg",
                "text_field_font"
              ]
            },
            {
              "type": "label",
              "title": "Surname:",
              "laying": "newRow",
              "properties": [
                "label_size"
              ]
            },
            {
              "type": "textField",
              "name": "surname_field",
              "laying": "row",
              "properties": [
                "text_field_size",
                "text_field_bg",
                "text_field_font"
              ]
            },
            {
              "type": "label",
              "title": "ID:",
              "laying": "newRow",
              "properties": [
                "label_size"
              ]
            },
            {
              "type": "textField",
              "name": "id_field",
              "laying": "row",
              "properties": [
                "text_field_size",
                "text_field_bg",
                "text_field_font"
              ]
            },
            {
              "type": "label",
              "title": "Phone:",
              "laying": "newRow",
              "properties": [
                "label_size"
              ]
            },
            {
              "type": "textField",
              "name": "phone_field",
              "laying": "row",
              "properties": [
                "text_field_size",
                "text_field_bg",
                "text_field_font"
              ]
            },
            {
              "type": "label",
              "title": "Request theme:",
              "laying": "newRow",
              "properties": [
                "label_size"
              ]
            },
            {
              "type": "multiList",
              "name": "requestTheme",
              "laying": "row",
              "values": [
                "rates",
                "deposit",
                "loans",
                "cash-back",
                "registration"
              ],
              "properties": [
                "list_size",
                "text_field_bg",
                "text_field_font"
              ]
            },
            {
              "type": "label",
              "title": "Additional info:",
              "laying": "newRow",
              "properties": [
                "label_size"
              ]
            },
            {
              "type": "textArea",
              "name": "additional_info",
              "laying": "row",
              "properties": [
                "text_area_size",
                "text_field_bg",
                "text_field_font"
              ]
            },
            {
              "type": "checkBox",
              "title": "Is a new customer?",
              "name": "is_new_customer",
              "values": [
                "true",
                "false"
              ],
              "properties": [
                "checkbox_size"
              ]
            },
            {
              "type": "slider",
              "name": "happiness_level",
              "values": [
                "perfect",
                "5",
                "good",
                "4",
                "average",
                "3",
                "not good",
                "2",
                "bad",
                "1"
              ],
              "properties": [
                "slider_size",
                "slider_fg"
              ]
            },
            {
              "laying": "row",
              "type": "button",
              "title": "Clean",
              "name": "clean",
              "properties": [
                "control_background",
                "control_foreground",
                "button_size"
              ]
            },
            {
              "laying": "row",
              "type": "button",
              "title": "Copy",
              "name": "copy",
              "properties": [
                "control_background",
                "control_foreground",
                "button_size"
              ]
            },
            {
              "laying": "row",
              "type": "button",
              "title": "Send",
              "name": "send",
              "properties": [
                "control_background",
                "control_foreground",
                "button_size"
              ]
            }
          ]
        }
      ]
    }
  ]
}